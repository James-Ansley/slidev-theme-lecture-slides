{
  "name": "slidev-theme-lecture-slides",
  "version": "0.0.10",
  "description": "No nonsense lecture slides",
  "license": "MIT",
  "author": {
    "name": "James Ansley"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/James-Ansley/slidev-theme-lecture-slides.git"
  },
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.23",
    "prism-theme-vars": "^0.2.5"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.23"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}